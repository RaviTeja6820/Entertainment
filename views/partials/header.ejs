<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:400,600,700" rel="stylesheet">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css">
        <link rel="shortcut icon" href="#" type="image/x-icon">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <script type="text/javascript" src="http://code.jquery.com/jquery-3.1.1.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
        <script src="/Scripts/dashScript.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="/stylesheets/style.css">
        <link rel="shortcut icon" href="/logo13.png" type="image/x-icon">
        <title>Entertainment // Search over Movies, Games, Comics, Manga, Anime, Actors, Favourate Characters etc...</title>
    </head>
    <!-- <body id="body"> -->
    <body>
        <!-- Navigation -->

      <div id="nav-pic" class="img-fluid">
            <a class="navbar-brand col-1" href="/" style="margin-top: 26vh; z-index: 1;">
                <img src="https://i.imgur.com/0Qjp6Kz.png" alt="" style="width: 100%; border-radius: 50%; border: 3px solid black; background-color: black; min-width: 70px; max-width: 80px;">
            </a>
      </div>
      <nav class="navbar navbar-expand-lg navbar-light" style="background-color: black; margin-bottom: 10px;">
            <ul class="navbar-nav mr-auto" style="margin-left: 80px;">
                <li class="nav-item active d-sm-block d-none" >
                  <a class="nav-link" href="/dashboard" style="color: white; font-weight: 800;">DashBoard <span class="sr-only">(current)</span></a>
                </li>
            </ul>
            <button class="navbar-toggler" style="margin-left: 80px;" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fas fa-bars" style="color: white; font-size: 2rem;"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav mr-auto"> 
                <li class="nav-item active d-sm-none" >
                  <a class="nav-link" href="/dashboard" style="color: white; font-weight: 800;">DashBoard <span class="sr-only">(current)</span></a>
                </li>
              </ul>
              <div class="form-inline my-2 my-lg-0">
                <ul class="navbar-nav mr-auto">
                <% if(!currentUser) { %>
                <li class="nav-item">
                  <form action="/login" method="POST" style="margin-bottom: 0; margin-right: 6px; padding: 0;">
                    <ul class="navbar-nav mr-auto">
                      <li class="mx-1">
                        <input type="text" name="username" class="form-control my-1" id="username" placeholder="UserID" style="height: 5vh;">
                      </li>
                      <li class="mx-1">
                        <input type="password" name="password" class="form-control my-1" id="password" placeholder="Password" style="height: 5vh;">
                      </li>
                      <li class="mx-1">
                          <button class="btn btn-sm btn-outline-light mr-1" style="padding: 2px 15px; border-radius: 15px; margin-top: 5px; font-size: 1rem; transition: all 0.5s;">Login <span class="sr-only">(current)</span></button>
                      </li>
                    </ul>
                  </form>
                </li>
                <li class="nav-item">
                  <form action="/signup" method="GET" style="margin-bottom: 0; padding: 0;">
                  <button class="nav-link btn btn-light btn-lg" style="padding: 2px 15px; border-radius: 15px; margin-top: 5px; color: black; font-size: 1rem;">SignUp</button>
                  </form>
                </li>
                <% } else { %>
                <li class="nav-item dropdown" >
                  <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      <i class="far fa-thumbs-up font-weight-bold" style="font-size: 1.5rem; color : white;"><i class="fas fa-caret-down ml-2"></i></i>
                  </a>
                  <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                    <% currentUser.likes.forEach( (el) => { %>
                      <a class="dropdown-item" href="<%= el.url %>" target="_blank"><%= el.name %></a>
                    <% }); %>
                </li>
                <li>
                    <h6 class="mr-2" style="color: white; margin-top: 0.6rem;">LoggedIn as <%= currentUser.username %></h6>
                </li>
                <li class="nav-item">
                  <form action="/logout" method="GET" style="margin-bottom: 0; padding: 0;">
                  <button class="nav-link btn btn-light btn-lg" style="padding: 2px 15px; border-radius: 15px; margin-top: 5px; color: black; font-size: 1rem;">LogOut</button>
                  </form>
                </li>
                  <% } %>
              </ul>
              </div>
              
            </div>
              <% if(currentUser) { %>
            <form action="/profile/<%= currentUser._id %>" method="GET" class="p-0 m-0">
              <button style="background: none; border: none; padding: 0;">
                <!-- <img src="data:image/png;base64," style="width: 7vh; margin-left: 2px; border-radius: 50%; border: 5px solid black; background-color: black; min-width: 50px; max-width: 70px;"> -->
                <img src="<%= currentUser.displayPicture %>" style="width: 7vh; margin-left: 2px; border-radius: 50%; border: 5px solid black; background-color: black; min-width: 50px; max-width: 70px;">
              </button>
            </form>
              <% } %>
          </nav>

          <% if(error && error.length > 0) { %>
            <div class="container">
                <div class="alert alert-danger" role="alert"><%= error %></div> 
            </div>
            <% } else if(success && success.length > 0) { %>
            <div class="container">
              <div class="alert alert-success" role="alert"><%= success %></div> 
            </div>
            <% } %>
            
            <div id="body">



          